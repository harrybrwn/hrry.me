{
  "name": "harrybrwn.com",
  "version": "0.0.1",
  "description": "My personal website",
  "scripts": {
    "test": "jest --rootDir . -c config/jest.config.ts",
    "test:ci": "jest --rootDir . -c config/jest.config.ts --coverage --runInBand --ci",
    "coverage": "x-www-browser .testing/jest/coverage/lcov-report/index.html",
    "build": "webpack --env prod --mode=production --progress",
    "build:ci": "webpack --env ci --mode=production --no-devtool --no-cache",
    "dev": "webpack -w --mode development --env dev --progress",
    "dev-build": "webpack --mode development --env dev",
    "clean": "rm -rf ./build",
    "serve": "webpack-dev-server --mode development --env dev --content-base frontend --progress --hot"
  },
  "engines": {
    "node": "16.13.x",
    "yarn": "1.22.x"
  },
  "main": "main.go",
  "repository": {
    "type": "git",
    "url": "github.com/harrybrwn/harrybrwn.com"
  },
  "author": "Harry Brown",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.0.0",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.4.5",
    "sass-loader": "^12.4.0",
    "sitemap-webpack-plugin": "^1.1.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0"
  }
}
