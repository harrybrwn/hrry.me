{
  "s3": {
    "buckets": [
      {
        "Name": "loki-logs"
      },
      {
        "Name": "docker-registry"
      },
      {
        "Name": "db-backups"
      },
      {
        "Name": "frontend",
        "policy": "static-files"
      },
      {
        "Name": "source"
      },
      {
        "Name": "files"
      },
      {
        "Name": "outline-wiki-data",
        "policy": "outline-bucket-access"
      },
      {
        "Name": "mastodon"
      },
      {
        "Name": "geoip"
      }
    ],
    "policies": {
      "loki": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "s3:DeleteObject",
              "s3:ListBucket"
            ],
            "Effect": "Allow",
            "Resource": ["arn:aws:s3:::loki-logs/*", "arn:aws:s3:::loki-logs"]
          },
          {
            "Action": ["s3:ListBucket"],
            "Effect": "Allow",
            "Resource": ["arn:aws:s3:::loki-logs"]
          }
        ]
      },
      "registry": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "s3:ListBucket",
              "s3:GetBucketLocation",
              "s3:ListBucketMultipartUploads"
            ],
            "Resource": ["arn:aws:s3:::docker-registry"]
          },
          {
            "Effect": "Allow",
            "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "s3:DeleteObject",
              "s3:ListMultipartUploadParts",
              "s3:AbortMultipartUpload"
            ],
            "Resource": ["arn:aws:s3:::docker-registry/*"]
          }
        ]
      },
      "backups": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": ["s3:PutObject"],
            "Resource": ["arn:aws:s3:::db-backups"]
          }
        ]
      },

      "static-files": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Principal": { "AWS": ["*"] },
            "Action": ["s3:GetBucketLocation", "s3:ListBucket"],
            "Resource": ["arn:aws:s3:::frontend"]
          },
          {
            "Effect": "Allow",
            "Principal": {
              "AWS": ["*"]
            },
            "Action": ["s3:GetObject"],
            "Resource": [
              "arn:aws:s3:::frontend/*",
              "arn:aws:s3:::frontend/latest/*",
              "arn:aws:s3:::frontend/dev/*",
              "arn:aws:s3:::frontend/git:*/*"
            ]
          },
          {
            "Effect": "Allow",
            "Principal": {
              "AWS": ["*"]
            },
            "Action": ["s3:ListBucket"],
            "Resource": ["arn:aws:s3:::frontend"],
            "Condition": {
              "StringEquals": {
                "s3:prefix": ["latest", "dev", "git:*"]
              }
            }
          }
        ]
      },

      "geoip-readonly": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": ["s3:GetBucketLocation"],
            "Resource": ["arn:aws:s3:::geoip"]
          },
          {
            "Effect": "Allow",
            "Action": ["s3:GetBucketLocation"],
            "Resource": ["arn:aws:s3:::files"]
          },
          {
            "Effect": "Allow",
            "Action": ["s3:GetObject", "s3:ListBucket"],
            "Resource": ["arn:aws:s3:::geoip/*"]
          }
        ]
      },
      "geoip-readwrite": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": ["s3:*"],
            "Resource": ["arn:aws:s3:::geoip"]
          },
          {
            "Effect": "Allow",
            "Action": ["s3:*"],
            "Resource": ["arn:aws:s3:::geoip/*"]
          }
        ]
      },

      "frontend-uploader": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": ["s3:PutObject", "s3:GetObject", "s3:DeleteObject"],
            "Resource": [
              "arn:aws:s3:::frontend/git:*/*",
              "arn:aws:s3:::frontend/latest/*"
            ]
          }
        ]
      },
      "file-access-user": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": ["s3:GetBucketLocation"],
            "Resource": ["arn:aws:s3:::files"]
          },
          {
            "Effect": "Allow",
            "Action": [
              "s3:ListBucket",
              "s3:GetObject",
              "s3:PutObject",
              "s3:DeleteObject"
            ],
            "Resource": ["arn:aws:s3:::files/*"]
          }
        ]
      },
      "outline": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "s3:ListBucket",
              "s3:GetBucketLocation",
              "s3:ListBucketMultipartUploads"
            ],
            "Resource": ["arn:aws:s3:::outline-wiki-data"]
          },
          {
            "Sid": "",
            "Effect": "Allow",
            "Action": [
              "s3:DeleteObject",
              "s3:PutObject",
              "s3:GetObject"
            ],
            "Resource": ["arn:aws:s3:::outline-wiki-data/*"]
          }
        ]
      },
      "outline-bucket-access": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Principal": { "AWS": ["*"] },
            "Action": ["s3:GetBucketLocation", "s3:ListBucket"],
            "Resource": ["arn:aws:s3:::outline-wiki-data"]
          },
          {
            "Effect": "Allow",
            "Principal": {
              "AWS": ["*"]
            },
            "Action": ["s3:GetObject", "s3:PutObject"],
            "Resource": [
              "arn:aws:s3:::outline-wiki-data/*"
            ]
          }
        ]
      },
      "mastodon": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "s3:ListBucket",
              "s3:GetBucketLocation",
              "s3:ListBucketMultipartUploads"
            ],
            "Resource": ["arn:aws:s3:::mastodon"]
          },
          {
            "Effect": "Allow",
            "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "s3:DeleteObject",
              "s3:ListMultipartUploadParts",
              "s3:AbortMultipartUpload"
            ],
            "Resource": ["arn:aws:s3:::mastodon/*"]
          }
        ]
      }
    },
    "groups": []
  },
  "db": {
    "port": "5432",
    "root_user": "harrybrwn",
    "Users": [],
    "Databases": [
      {
        "Name": "grafana",
        "Owner": "grafana"
      },
      {
        "Name": "hr",
        "Owner": "harrybrwn"
      },
      {
        "Name": "movies",
        "Owner": "harrybrwn"
      },
      {
        "Name": "hydra",
        "Owner": "hydra"
      },
      {
        "Name": "outline",
        "Owner": "outline"
      },
      {
        "Name": "mastodon",
        "Owner": "mastodon"
      }
    ],
    "migrations": {
      "api": {
        "database": "harrybrwn_api",
        "source": "file://./db/migrations/api"
      },
      "hr": {
        "database": "hr",
        "source": "file://./db/migrations/data-analyst-roadmap_hr"
      },
      "movies": {
        "database": "movies",
        "source": "file://./db/migrations/data-analyst-roadmap_movies"
      },
      "hooks": {
        "database": "harrybrwn_hooks",
        "source": "file://./db/migrations/hooks"
      }
    }
  }
}
