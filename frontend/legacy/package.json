{
  "name": "@hrry.me/legacy",
  "version": "0.0.1",
  "description": "My personal website",
  "private": true,
  "scripts": {
    "test": "jest --rootDir ../ -c jest.config.mjs",
    "test:ci": "jest --rootDir ../ -c jest.config.mjs --coverage --runInBand --ci",
    "coverage": "x-www-browser .cache/test/jest/coverage/lcov-report/index.html",
    "build": "webpack -c webpack.config.js --env prod --mode=production --progress",
    "build:ci": "webpack -c webpack.config.js --env ci --mode=production --no-devtool --no-cache",
    "dev": "webpack -c webpack.config.js -w --mode development --env dev --progress",
    "start": "webpack-dev-server -c webpack.config.js --mode=development"
  },
  "main": "main.ts",
  "repository": {
    "type": "git",
    "url": "github.com/harrybrwn/harrybrwn.com",
    "directory": "frontend/legacy"
  },
  "author": "Harry Brown",
  "license": "UNLICENSED",
