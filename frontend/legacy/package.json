    "@babel/preset-typescript": "^7.16.7",
    "@hrry.me/api": "^0.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
